{
  "name": "questioner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index",
    "coverage": "nyc --reporter=html --reporter=text-lcov npm test",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && cd dist && mkdir api && cd ..&& babel index.js -s -d dist && babel ./api -d dist/api",
    "test": "mocha test --require babel-core/register --timeout 10000 --exit",
    "dev": "babel-watch index.js",
    "test-build": "cd dist && mkdir testbabel test/ -d dist && test-start",
    "dev-test": "mocha 10000",
    "test-mocha": "mocha --timeout 5000"
  },
  "nyc": {
    "register": "babel-register",
    "include": [
      "**/*.js",
      "./*.js"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/K3beros/questioner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/K3beros/questioner/issues"
  },
  "homepage": "https://github.com/K3beros/questioner#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "moment": "^2.23.0",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.3.0"
  }
}
